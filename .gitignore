# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/storage
/config/master.key
/config/credentials/production.key
/public
.byebug_history
# Archivos de logs
/log/*
!/log/.keep

# Archivos temporales
/tmp/*
!/tmp/.keep

# Backups y scripts locales
/backup
